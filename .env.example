# Configuración de MySQL remoto
MYSQL_HOST=tu_host_mysql.com
MYSQL_PORT=3306
MYSQL_DATABASE=tu_base_de_datos
MYSQL_USER=tu_usuario
MYSQL_PASSWORD=tu_contraseña

# Configuración de aplicación
FLASK_ENV=production
SECRET_KEY=genera_una_clave_secreta_segura_aqui

# Configuración del proxy API (opcional)
PROXY_API_KEY=tu_clave_api_segura
USE_HTTP_PROXY=false
